<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.CourseOpen" table="course_open" schema="public" catalog="StudentDB">
        <id name="ids">
            <column name="ids" sql-type="integer"/>
            <generator class="identity"/>
        </id>
        <property name="startDay">
            <column name="start_day" sql-type="date"/>
        </property>
        <property name="endDay">
            <column name="end_day" sql-type="date"/>
        </property>
        <many-to-one class="hibernate.Semester" name="id" lazy="false" cascade="save-update">
            <column name="id" sql-type="int"/>
        </many-to-one>
        <many-to-one class="hibernate.Subject" name="courseId" lazy="false" cascade="save-update">
            <column name="course_id" sql-type="varchar(10)" length="10"/>
        </many-to-one>
    </class>
</hibernate-mapping>